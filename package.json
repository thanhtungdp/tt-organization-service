{
  "name": "clean-service",
  "version": "0.0.0",
  "main": "src/index.js",
  "scripts": {
    "dev": "NODE_PATH=./src micro-dev src/boot.js --ignore .idea --ignore logs",
    "start": "NODE_ENV=production NODE_PATH=./src micro",
    "test": "NODE_PATH=./src ava",
    "fixcode": "prettier-standard src/**/*.js && standard --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run fixcode && npm run test"
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "xo": {
    "extends": "prettier"
  },
  "dependencies": {
    "@bit/tungtung.micro.components.amqp": "^0.0.1",
    "@bit/tungtung.micro.components.logger": "^0.0.1",
    "@bit/tungtung.micro.components.micro-boom": "^0.0.1",
    "@bit/tungtung.micro.components.micro-crud": "^0.0.1",
    "@bit/tungtung.micro.components.micro-joi": "^0.0.1",
    "@bit/tungtung.micro.components.mongo": "^0.0.3",
    "ava": "^0.25.0",
    "boom": "^7.2.0",
    "chalk": "^2.4.1",
    "dotenv": "^6.0.0",
    "faker": "^4.1.0",
    "joi": "^13.4.0",
    "micro": "latest",
    "microrouter": "^3.1.3",
    "mongoose": "^5.1.7",
    "prettier-standard": "^8.0.1",
    "url": "^0.11.0"
  },
  "license": "MIT",
  "devDependencies": {
    "husky": "^1.0.0-rc.13",
    "micro-dev": "latest",
    "prettier": "1.5.3",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "standard": "^11.0.1",
    "test-listen": "1.0.2"
  }
}
